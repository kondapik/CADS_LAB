PROG=testLists
RM=rm all

all: $(PROG).cpp
	@g++ -std=c++11 -Wall -pthread $(PROG).cpp -o $(PROG)
	./$(PROG) 2 10 1024

	./$(PROG) 2 50 1024

	./$(PROG) 2 90 1024

	./$(PROG) 4 10 1024

	./$(PROG) 4 50 1024

	./$(PROG) 4 90 1024

	./$(PROG) 8 10 1024

	./$(PROG) 8 50 1024

	./$(PROG) 8 90 1024

	./$(PROG) 16 10 1024

	./$(PROG) 16 50 1024

	./$(PROG) 16 90 1024

	./$(PROG) 32 10 1024

	./$(PROG) 32 50 1024

	./$(PROG) 32 90 1024

.PHONY: clean
clean:
	$(RM) $(PROG)